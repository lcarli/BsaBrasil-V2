                        <div>
                            <i class="fa fa-map-marker"></i>
                            <p>Alameda Rio Negro, 503 - 5º Andar<br>Barueri, SP 06454-040</p>
                        </div>

                        <div>
                            <i class="fa fa-envelope"></i>
                            <p>administrativo@bsabrasil.com.br</p>
                        </div>

                        <div>
                            <i class="fa fa-phone"></i>
                            <p>+55 11 4382 2551</p>
                        </div>

                    </div>
                </div>

                <div class="col-md-5 col-md-push-2">
                    <div class="form">
                        <div id="sendmessage">@Localizer["Sua mensagem foi enviada. Obrigado!"]</div>
                        <div id="errormessage"></div>
                        <form action="" method="post" role="form" class="contactForm">
                            <div class="form-group">
                                <input type="text" name="name" class="form-control" id="name" placeholder="@Localizer["Seu nome"]" data-rule="minlen:4" data-msg="@Localizer["Por favor, digite ao menos 4 caracteres"]" />
                                <div class="validation"></div>
                            </div>
                            <div class="form-group">
                                <input type="email" class="form-control" name="email" id="email" placeholder="@Localizer["Seu e-mail"]" data-rule="email" data-msg="@Localizer["Por favor, digite um e-mail válido"]" />
                                <div class="validation"></div>
                            </div>
                            <div class="form-group">
                                <input type="text" class="form-control" name="subject" id="subject" placeholder="@Localizer["Assunto"]" data-rule="minlen:4" data-msg="@Localizer["Por favor, digite ao menos 8 caracteres como assunto"]" />
                                <div class="validation"></div>
                            </div>
                            <div class="form-group">
                                <textarea class="form-control" name="message" rows="5" data-rule="required" data-msg="@Localizer["Por favor, escreve algo para nós"]" placeholder="@Localizer["Mensagem"]"></textarea>
                                <div class="validation"></div>
                            </div>
                            <div class="text-center"><button type="submit">@Localizer["Enviar Mensagem"]</button></div>
                        </form>
                    </div>
                </div>

            </div>
        </div>
    </section>

