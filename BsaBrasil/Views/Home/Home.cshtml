    <!--Custom Script-->
    <script type="text/javascript">
        function move() {
            //C#
            var elem = document.getElementById("BarC");
            var width = 1;
            var id = setInterval(frame, 20);
            function frame() {
                if (width >= 95) {
                    clearInterval(id);
                } else {
                    width++;
                    elem.style.width = width + '%';
                }
            }

            //ANGULAR
            var elem2 = document.getElementById("BarAngular");
            var width2 = 1;
            var id2 = setInterval(frame2, 20);
            function frame2() {
                if (width2 >= 45) {
                    clearInterval(id2);
                } else {
                    width2++;
                    elem2.style.width = width2 + '%';
                }
            }

            //.NET
            var elem3 = document.getElementById("BarNET");
            var width3 = 1;
            var id3 = setInterval(frame3, 20);
            function frame3() {
                if (width3 >= 95) {
                    clearInterval(id3);
                } else {
                    width3++;
                    elem3.style.width = width3 + '%';
                }
            }

            //PHP
            var elem4 = document.getElementById("BarPHP");
            var width4 = 1;
            var id4 = setInterval(frame4, 20);
            function frame4() {
                if (width4 >= 55) {
                    clearInterval(id4);
                } else {
                    width4++;
                    elem4.style.width = width4 + '%';
                }
            }

            //JAVA
            var elem5 = document.getElementById("BarJava");
            var width5 = 1;
            var id5 = setInterval(frame5, 20);
            function frame5() {
                if (width5 >= 75) {
                    clearInterval(id5);
                } else {
                    width5++;
                    elem5.style.width = width5 + '%';
                }
            }

            //Oracle
            var elem7 = document.getElementById("BarOracle");
            var width7 = 1;
            var id7 = setInterval(frame7, 20);
            function frame7() {
                if (width7 >= 90) {
                    clearInterval(id7);
                } else {
                    width7++;
                    elem7.style.width = width7 + '%';
                }
            }

            //SQL
            var elem6 = document.getElementById("BarSQL");
            var width6 = 1;
            var id6 = setInterval(frame6, 20);
            function frame6() {
                if (width6 >= 85) {
                    clearInterval(id6);
                } else {
                    width6++;
                    elem6.style.width = width6 + '%';
                }
            }
        }
    </script>
